{
  "parser": "babel-eslint",
  "extends": ["plugin:flowtype/recommended", "eslint:recommended"],
  "plugins": ["flowtype"],
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "warn"
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
}
